{
    "name": "ronasit/laravel-chat",
    "description": "This package implements chat functionality based on push notifications.",
    "keywords": [
        "laravel",
        "chat"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ronas IT",
            "homepage": "https://github.com/RonasIT/laravel-chat/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ronasit/laravel-helpers": "^2.1",
        "alymosul/laravel-exponent-push-notifications": "^2.4",
        "phpunit/phpunit": "^9.0",
        "ronasit/laravel-swagger": ">=2.0.4-beta",
        "laravel/framework": "^8.0",
        "php-open-source-saver/jwt-auth": "^2.0",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
